# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'EasySettings' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'IGListKit'
    pod 'SnapKit'
    pod 'SwifterSwift'
    pod 'SwipeCell'
    pod 'AdvancedSegmentedControl'
    pod 'UIImageColors'
  # Pods for EasySettings

  target 'EasySettingsTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'IGListKit'
  pod 'SnapKit'
  pod 'SwifterSwift'
  pod 'SwipeCell'
  pod 'AdvancedSegmentedControl'
  pod 'UIImageColors'
  end

end
